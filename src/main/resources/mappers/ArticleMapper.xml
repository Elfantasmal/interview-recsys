<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rrdssfgcs.interviewcheatsheet.dao.ArticleDao">
    <select id="selectArticleByDocid" parameterType="String"
            resultType="com.rrdssfgcs.interviewcheatsheet.entity.Article">
        select * from rrdssfgcs.article where docid=#{docid}
    </select>
    <insert id="insertArticle">
        insert into rrdssfgcs.article (docid,title,content,author,source,url,update_time,insert_time)
        values (#{docid},#{title},#{content},#{author},#{source},#{url},#{update_time},#{insert_time})
    </insert>
</mapper>